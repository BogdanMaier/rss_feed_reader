{
  "sourceMaps": "both",
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "syntax-async-functions"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "source-map-support"
      ],
      "ignore": [
        "**/__tests__/**"
      ]
    }
  }
}